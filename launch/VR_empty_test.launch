<?xml version="1.0"?>

<launch>

	 <!-- "RunViveTracker.sh" -->
         <!--node name="Vrui_server" pkg="vrui_mdf" type="Vrui_server"/-->

	 <!-- "Gazebo" -->

  	 <!-- Start Gazebo with wg world running in (max) realtime -->

  	 <!-- Spawn simulated quadrotor uav -->

  <arg name="world" default="worlds/empty.world"/>
  <arg name="gui" default="true"/>
  <arg name="args" default=""/>
  
  <param name="/use_sim_time" value="true" />

  <node name="gazebo" pkg="gazebo_ros" type="gzserver" args="$(arg world) $(arg args)" respawn="false" output="screen"/>
  

 



	  <!-- talk to tracking server -->
          <node name="opentracking" pkg="openvr_headset_ros" type="opentracking"/>

          <!-- display for HMD -->
          <node name="imagesub" pkg="openvr_headset_ros" type="imagesub"/>


          <!-- test -->
	  <node name="spawn_model_cube" pkg="gazebo_ros" type="spawn_model"
	  args="-file ~/catkin_ws/models/cube_black/model.sdf -sdf -model cube_black 
	  -x 0 -y 0 -z 1" output="screen" />	  

	  <node name="spawn_model_cube1" pkg="gazebo_ros" type="spawn_model"
	  args="-file ~/catkin_ws/models/cube_black/model.sdf -sdf -model cube_black_0 
	  -x 0 -y 0 -z 1" output="screen" />
	  
	<group if="$(arg gui)">
    <node name="gazebo_gui" pkg="gazebo_ros" type="gzclient" respawn="false" output="screen"/>
  </group>

</launch>
